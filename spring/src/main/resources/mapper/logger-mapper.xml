<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hana1piece.logger.model.mapper.LoggerMapper">

    <insert id="insertLog" parameterType="com.hana1piece.logger.model.vo.Log">
        insert into exception_log(log_code, program, msg, note)
        values(#{logCode}, #{program}, #{msg}, #{note})
    </insert>

</mapper>